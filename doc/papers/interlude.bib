	Pages = {43--51},
	Publisher = {CIEREC},
	Title = {Towards dynamic and animated music notation using {INS}core},
	Year = {2017}}

@inproceedings{fober17b,
	Abstract = {INScore est un environnement pour la conception de partition interactives augment{\'e}es, tourn{\'e} vers des usages non conventionnels de la notation musicale, sans exclure pour autant les approches classiques. Dans cet environnement, bien que tous les objets de la partition aient une dimension temporelle, le temps reste fixe i.e. que la date (ou la dur{\'e}e) d'un objet ne change pas, sauf {\`a} r{\'e}ception d'un message qui ne peut {\^e}tre produit que de mani{\`e}re externe ou {\'e}v{\'e}nementielle. INScore n'inclut donc pas de gestionnaire du temps au sens classique du terme. A l'origine, ce choix a {\'e}t{\'e} dict{\'e} par le fait que le syst{\`e}me fut con{\c c}u pour des usages coupl{\'e}s avec des logiciels de production sonore (e.g. Max/MSP, Pure Data), qui ont des contraintes de temps-r{\'e}el plus strictes que l'environnement graphique d'INScore. Toutefois, la n{\'e}cessit{\'e} d'introduire un temps dynamique a progressivement {\'e}merg{\'e}, conduisant {\`a} un mod{\`e}le original, {\`a} la fois {\'e}v{\'e}nementiel et continu. C'est ce mod{\`e}le qui est pr{\'e}sent{\'e} et ses propri{\'e}t{\'e}s dans l'environnement d'INScore.},
