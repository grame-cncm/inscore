/ITL/scene/curve set curve 0. 0. 0.5 0.5 0.5 -0.75 0.75 0.5;
/ITL/scene/curve x 0.38;

/ITL/scene/curve z 1;

/ITL/scene/polygon set polygon 0.5 0 0 0.5 0.5 0.5;
/ITL/scene/polygon x -0.25;
/ITL/scene/polygon y -0.25;
/ITL/scene/polygon color 255 112 0;
/ITL/scene/polygon get color;

/ITL/scene/rect set rect 0.3 0.5;
/ITL/scene/rect y -0.5;
/ITL/scene/rect x 0.15;
/ITL/scene/rect color 0 200 200 150;
/ITL/scene/rect penWidth 1;
/ITL/scene/rect penStyle dashDot;